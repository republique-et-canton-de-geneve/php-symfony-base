{
    "name": "republique-et-canton-de-geneve/response-headers-bundle",
    "version" : "1.0",
    "description": "A Symfony bundle to easily send headers in your HTTP response",
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.4",
        "symfony/dependency-injection": ">=6.4",
        "symfony/http-kernel" : ">=6.4",
        "symfony/config" : ">=6.4",
        "symfony/expression-language": ">=6.4"
    },
     "require-dev": {
        "phpunit/phpunit": "^12.4"
    },
    "license": "MIT",
    "keywords": ["bundle", "symfony", "response", "header", "headers","http", "condtion" ],
    "autoload": {
        "psr-4": {
            "EtatGeneve\\ResponseHeadersBundle\\": "src/"
        }
    },
    "authors": [
        {
            
            "name": "Michel Bobillier aka Athos99",
            "email": "athos99@bluewin.ch",
            "role": "Senior Lead Developer"


        }
    ],
    "minimum-stability": "stable"
}
