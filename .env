### choose beetwen SQLite or PostgreSQL with SSL

#APP_DATABASE_URL=sqlite3:///%kernel.project_dir%/data/database.sqlite
#APP_DATABASE_URL_CLI=sqlite3:///%kernel.project_dir%/data/database.sqlite

# Connect to PostgreSQL with SSL, using client certificates 
# The certificates are different depending on CLI mode (Console commands) or Web mode (www-data user)
# look at src/CliEnvVarPocessor.php to see how to handle that
APP_DATABASE_URL="postgresql://user:root@postgres:5432/php-symfony-base?serverVersion=17&charset=utf8&sslmode=verify-full&sslrootcert=/var/certs/www-data/postgres/rootCA.crt&sslcert=/var/certs/www-data/postgres/client.crt&sslkey=/var/certs/www-data/postgres/client.key"
APP_DATABASE_URL_CLI="postgresql://user:root@postgres:5432/php-symfony-base?serverVersion=17&charset=utf8&sslmode=verify-full&sslrootcert=/var/certs/root/postgres/rootCA.crt&sslcert=/var/certs/root/postgres/client.crt&sslkey=/var/certs/root/postgres/client.key"

